   3.9031854607388773`*^9}, {3.905358887040047*^9, 3.905358891225012*^9}, {
   3.907161370767453*^9, 3.907161371892496*^9}, {3.907817203385817*^9, 
   3.907817203622153*^9}, {3.908397850368978*^9, 3.908397850605451*^9}, {
