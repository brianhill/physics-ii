          RowBox[{"-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"x", "-", 
              SubscriptBox["x", "0"]}], ")"}], "2"]}], "/", 
          SuperscriptBox["a", "2"]}]}]], "d", "\[VeryThinSpace]", "x"}]}]}]}],
    TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[HBar]", "2"], 
      SuperscriptBox["a", "4"]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], " ", 
      RowBox[{
       SuperscriptBox["N", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{
           SuperscriptBox[
            SubscriptBox["p", "0"], "2"], 
           SuperscriptBox["a", "4"]}], 
          SuperscriptBox["\[HBar]", "2"]], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"x", "-", 
            SubscriptBox["x", "0"]}], ")"}], "2"]}], ")"}], 
       SuperscriptBox["e", 
        RowBox[{" ", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"x", "-", 
              SubscriptBox["x", "0"]}], ")"}], "2"]}], "/", 
          SuperscriptBox["a", "2"]}]}]], "d", "\[VeryThinSpace]", "x"}]}]}]}],
    TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox[
      SuperscriptBox["\[HBar]", "2"], 
      SuperscriptBox["a", "4"]], 
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       RowBox[{"-", "\[Infinity]"}], "\[Infinity]"], " ", 
      RowBox[{
       SuperscriptBox["N", "2"], " ", 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          RowBox[{
           SuperscriptBox[
            SubscriptBox["p", "0"], "2"], 
           SuperscriptBox["a", "4"]}], 
          SuperscriptBox["\[HBar]", "2"]], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"x", "-", 
            SubscriptBox["x", "0"]}], ")"}], "2"]}], ")"}], 
       SuperscriptBox["e", 
        RowBox[{" ", 
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"x", "-", 
              SubscriptBox["x", "0"]}], ")"}], "2"]}], "/", 
          SuperscriptBox["a", "2"]}]}]], "d", "\[VeryThinSpace]", "x"}]}]}]}],
    TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       SuperscriptBox["a", "4"]], 
      FractionBox[
       RowBox[{
        SuperscriptBox[
         SubscriptBox["p", "0"], "2"], 
        SuperscriptBox["a", "4"]}], 
       SuperscriptBox["\[HBar]", "2"]]}], "+", 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[HBar]", "2"], 
       SuperscriptBox["a", "4"]], 
      FractionBox["1", "2"], 
      SuperscriptBox["a", "2"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"=", 
