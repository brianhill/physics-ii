       RowBox[{"\[Mu]", "-", 
        RowBox[{"5", "\[Sigma]"}]}], ",", 
       RowBox[{"\[Mu]", "+", 
        RowBox[{"5", "\[Sigma]"}]}]}], "}"}], ",", 
     RowBox[{"PlotRange", "->", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\[Mu]", "-", 
           RowBox[{"5", "\[Sigma]"}]}], ",", 
          RowBox[{"\[Mu]", "+", 
           RowBox[{"5", "\[Sigma]"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0.2"}], "}"}]}], "}"}]}]}], "]"}]}]}]], "Input",
