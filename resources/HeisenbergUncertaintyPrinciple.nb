Cell[BoxData[
 RowBox[{" ", 
  RowBox[{
   RowBox[{"\[Mu]", "=", "5"}], ";", " ", 
   RowBox[{"\[Sigma]", "=", "2"}], ";", 
   RowBox[{"Plot", "[", 
    RowBox[{
     FormBox[
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SqrtBox[
          RowBox[{"2", "\[Pi]"}]], "\[Sigma]"}]], 
