Cell[TextData[{
 "Problem 3 of Problem Set 19, was to solve the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Omega]", "1"], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c704e801-9fd4-482e-9a04-747e754d99e8"],
 " case. You found:\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["\[Psi]", "+"], "(", "t", ")"}]},
       {
        RowBox[{
         SubscriptBox["\[Psi]", "-"], "(", "t", ")"}]}
      }], ")"}], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         RowBox[{
          SubscriptBox["\[Psi]", "+"], "(", "0", ")"}], 
         SuperscriptBox["e", 
          RowBox[{" ", 
           RowBox[{
            FractionBox["i", "2"], 
            SubscriptBox["\[Omega]", "0"], "t"}]}]]}]},
       {
        RowBox[{
         RowBox[{
          SubscriptBox["\[Psi]", "-"], "(", "0", ")"}], 
         SuperscriptBox["e", 
          RowBox[{" ", 
           RowBox[{
            RowBox[{"-", 
             FractionBox["i", "2"]}], 
            SubscriptBox["\[Omega]", "0"], "t"}]}]]}]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "88c9ced0-47a6-4eeb-ba34-d515284f2cc4"]
}], "Text",
 CellChangeTimes->{{3.92223651350019*^9, 3.92223685901387*^9}, {
