  FormBox[
   RowBox[{
    SubscriptBox["\[Omega]", "1"], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "879dcae0-a8ac-479e-af12-07c3a6a42d3d"],
