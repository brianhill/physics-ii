  3.922238137403948*^9}, {3.9222387469032297`*^9, 3.922238800980549*^9}, {
  3.922238859741473*^9, 3.922238913899221*^9}, {3.922238943982003*^9, 
  3.922238983861414*^9}, {3.922239171011537*^9, 3.922239485052639*^9}, {
