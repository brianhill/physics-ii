163d29c94101"],

Cell[TextData[{
 "On p. 160, Moore told us\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "n"], " ", "=", 
    RowBox[{"\[HBar]", "\[VeryThinSpace]", 
     RowBox[{"\[Omega]", "(", 
      RowBox[{"n", "+", 
       FractionBox["1", "2"]}], ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "685becc0-4326-48b5-b27f-0b4e782d1238"],
 "\n\nFor ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e04ca94c-1f72-4450-87d6-e3c787d2d4a3"],
 " even, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Psi]", "n"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        SubscriptBox["c", "0"], "+", 
        SuperscriptBox[
         RowBox[{
          SubscriptBox["c", "2"], "(", 
          RowBox[{"b", "\[VeryThinSpace]", "x"}], ")"}], "2"], "+"}], "..."}],
       "]"}], 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"b", "\[VeryThinSpace]", "x"}], ")"}], "2"]}], "/", 
       "2"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "44e07d82-a542-4cfe-9f0f-0745a7521d8d"],
 "\n\nFor ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8395e6d3-6900-4e58-802f-f93d24114cda"],
 " odd, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Psi]", "n"], "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["c", "1"], "b", "\[VeryThinSpace]", "x"}], "+", 
        SuperscriptBox[
         RowBox[{
          SubscriptBox["c", "3"], "(", 
          RowBox[{"b", "\[VeryThinSpace]", "x"}], ")"}], "3"], "+"}], "..."}],
       "]"}], 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"b", "\[VeryThinSpace]", "x"}], ")"}], "2"]}], "/", 
       "2"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "36c73212-d6b1-4880-94a3-77d44d3a73e5"],
 "\n\nwhere there is some whacky recursion relation for the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", 
    RowBox[{"k", "+", "2"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cfb92440-fa42-4f26-8817-9e63af8d1ee6"],
 " in terms of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "k"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "718cafeb-dad4-4f0f-9898-d959e5c82dbf"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    SqrtBox[
     FractionBox[
      RowBox[{"m", "\[VeryThinSpace]", "\[Omega]"}], "\[HBar]"]]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e7da7db2-857a-4ba1-8a26-b799a897989c"],
 "."
}], "Text",
 CellChangeTimes->{{3.920310595255248*^9, 3.920310683530747*^9}, {
