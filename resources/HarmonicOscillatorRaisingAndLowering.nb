  FormatType->TraditionalForm,ExpressionUUID->
  "728de509-16ee-4619-9978-da4c894e1332"]
}], "Subsection",
 CellChangeTimes->{{3.920310155034218*^9, 3.920310163989311*^9}, {
  3.920310462568035*^9, 3.920310466416088*^9}, {3.920419866279635*^9, 
  3.920419866876333*^9}, {3.920420043877632*^9, 3.9204200642722178`*^9}, {
  3.920420893049715*^9, 
  3.920420935834853*^9}},ExpressionUUID->"d9af8377-7655-4ee4-93de-\
359081d44838"],

Cell[TextData[{
 "One thing we can do to make the equation slightly tidier is to define ",
 Cell[BoxData[
  FormBox["\[Omega]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "771e40b4-d993-46b4-9712-ac7f00016ca3"],
 " by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["k", 
     StyleBox["S",
      FontSlant->"Plain"]], "=", 
    RowBox[{"m", "\[VeryThinSpace]", 
     SuperscriptBox["\[Omega]", "2"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7b4d01a9-17e2-4283-8fd1-eff18e8ca24f"],
 ". Then the time-independent equation is\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "n"], 
     RowBox[{
      SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox["\[HBar]", "2"], 
        RowBox[{"2", "m"}]]}], 
      StyleBox[
       FractionBox[
        RowBox[{
         SuperscriptBox["d", "2"], 
         RowBox[{
          SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], 
        RowBox[{"d", "\[VeryThinSpace]", 
         SuperscriptBox["x", "2"]}]],
       FontSize->18]}], "+", 
     RowBox[{
      FractionBox["1", "2"], "m", "\[VeryThinSpace]", 
      SuperscriptBox["\[Omega]", "2"], 
      SuperscriptBox["x", "2"], 
      RowBox[{
       SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "014fa1cb-14a6-4ec3-bd5b-8dbc4088c5b9"],
 "\n\nWe can measure the energy ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "n"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7257c9f0-f79e-4453-a141-d331dccc5b42"],
 " in units of ",
 Cell[BoxData[
  FormBox["\[HBar]\[Omega]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e832e26c-66b8-41b4-9bfb-4a9d2f6e86a7"],
 ". Then we have\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "n"], " ", 
     FormBox["\[HBar]\[Omega]",
      TraditionalForm], 
     RowBox[{
      SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        SuperscriptBox["\[HBar]", "2"], 
        RowBox[{"2", "m"}]]}], 
      StyleBox[
       FractionBox[
        RowBox[{
         SuperscriptBox["d", "2"], 
         RowBox[{
          SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], 
        RowBox[{"d", "\[VeryThinSpace]", 
         SuperscriptBox["x", "2"]}]],
       FontSize->18]}], "+", 
     RowBox[{
      FractionBox["1", "2"], "m", "\[VeryThinSpace]", 
      SuperscriptBox["\[Omega]", "2"], 
      SuperscriptBox["x", "2"], 
      RowBox[{
       SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6a96dc7b-7f59-4db2-bf10-1d64b7f847fd"],
 "\n\nor\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "n"], " ", 
     RowBox[{
      SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], "=", 
    RowBox[{
     FractionBox["1", "2"], "[", 
     RowBox[{
      RowBox[{
       RowBox[{"-", 
        FractionBox["\[HBar]", "m\[Omega]"]}], 
       StyleBox[
        FractionBox[
         RowBox[{
          SuperscriptBox["d", "2"], 
          RowBox[{
           SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}], 
         RowBox[{"d", "\[VeryThinSpace]", 
          SuperscriptBox["x", "2"]}]],
        FontSize->18]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"m", "\[VeryThinSpace]", "\[Omega]"}], "\[HBar]"], 
       SuperscriptBox["x", "2"], 
       RowBox[{
        SubscriptBox["\[Psi]", "n"], "(", "x", ")"}]}]}], "]"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "49bcf259-2359-4c96-9865-114b3ac6e1d9"]
}], "Text",
 CellChangeTimes->{{3.920310595255248*^9, 3.920310683530747*^9}, {
