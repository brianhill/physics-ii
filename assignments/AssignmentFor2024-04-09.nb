   3.919274149001862*^9, {3.919714558053597*^9, 3.919714660184144*^9}, {
   3.919714692696037*^9, 3.919714694644412*^9}, 3.91971475412458*^9, {
   3.91971537876573*^9, 3.9197153804875*^9}, {3.919871476745599*^9, 
   3.919871559713085*^9}, {3.919871698776689*^9, 3.919871698799308*^9}, {
   3.919871817897055*^9, 3.919871843410635*^9}, {3.919958969909013*^9, 
   3.919958969993247*^9}, {3.92098520257545*^9, 3.920985204239929*^9}, {
   3.920985288120044*^9, 3.920985292773527*^9}, {3.920985566765376*^9, 
   3.9209855762825546`*^9}, {3.920985681427232*^9, 3.920985683676508*^9}, {
   3.9211552050430393`*^9, 3.921155273132989*^9}, {3.92149856587326*^9, 
   3.9214985933896112`*^9}, {3.921498637989336*^9, 3.921498723164444*^9}, {
